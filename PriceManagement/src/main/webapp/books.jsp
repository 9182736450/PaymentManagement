<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<title>Book Store</title>
<style>
/* Add the CSS styles */
.book-store {
	display: flex;
	flex-wrap: wrap;
	justify-content: center;
}

.book {
	width: 300px;
	height: 400px;
	margin: 20px;
	border: 1px solid gray;
	border-radius: 10px;
	text-align: center;
}

.book-image {
	width: 100%;
	height: 200px;
	background-position: center;
	background-repeat: no-repeat;
	background-size: cover;
	border-top-left-radius: 10px;
	border-top-right-radius: 10px;
}

.book-title {
	font-size: 24px;
	font-weight: bold;
	margin: 20px 0;
}

.book-author {
	font-size: 18px;
	margin: 10px 0;
}

.book-price {
	font-size: 18px;
	font-weight: bold;
	margin: 10px 0;
}

.add-to-cart-button {
	padding: 10px 20px;
	border: 1px solid gray;
	border-radius: 10px;
	cursor: pointer;
	background-color: lightgray;
	margin-bottom: 20px;
}

.cart {
	width: 400px;
	height: 500px;
	background-color: lightgray;
	border: 1px solid gray;
	border-radius: 10px;
	position: fixed;
	right: 20px;
	bottom: 20px;
	padding: 20px;
	text-align: left;
	display: none;
}

.cart-header {
	font-size: 24px;
	font-weight: bold;
	margin-bottom: 20px;
}

.cart-item {
	display: flex;
	justify-content: space-between;
	margin-bottom: 20px;
}

.cart-item-title {
	font-size: 18px;
}

.cart-item-price {
	font-size: 18px;
	font-weight: bold;
}

.checkout-button {
	padding: 10px 20px;
	border: 1px solid gray;
	border-radius: 10px;
	cursor: pointer;
	background-color: lightgray;
	margin-top: 20px;
	display: none;
}

.open-cart-button {
	padding: 10px 20px;
	border: 1px solid gray;
	border-radius: 10px;
	cursor: pointer;
	background-color: lightgray;
	position: fixed;
	right: 20px;
	bottom: 20px;
}
</style>

</head>
<body>
	<div class="book-store">
		<!-- Add some sample books -->
		<div class="book" data-title="Book 1" data-author="Author 1"
			data-price="10">
			<div class="book-image"
				style="background-image: url(https://4.imimg.com/data4/VD/CW/MY-9375281/computer-science-with-c-class-xi-500x500.jpg)"></div>
			<div class="book-title">C++ Programming Language</div>
			<div class="book-author">Margaret Ellis</div>
			<div class="book-price">$60</div>
			<a href="/buybooks">Buy</a>
		</div>
		<div class="book" data-title="Book 2" data-author="Author 2"
			data-price="20">
			<div class="book-image"
				style="background-image: url(https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTqr9hILyn2P-jymbeDTBHqXTGyO69unvayMA&usqp=CAU)"></div>
			<div class="book-title">Java Programming Language</div>
			<div class="book-author">Bjarne Stroustrup</div>
			<div class="book-price">$70</div>
			<a href="/buybooks">Buy</a>
		</div>

		<div class="book" data-title="Book 3" data-author="Author 3"
			data-price="30">
			<div class="book-image"
				style="background-image: url(data:image/jpeg;base64,)"></div>
			<div class="book-title">Python Programming Language</div>
			<div class="book-author">Guido van Rossum</div>
			<div class="book-price">$20</div>
			<a href="/buybooks">Buy</a>
		</div>

		<div class="book" data-title="Book 4" data-author="Author 3"
			data-price="30">
			<div class="book-image"
				style="background-image: url(https://qph.cf2.quoracdn.net/main-qimg-0e2d8c43a756a388a1617cbff2f3af53.webp)"></div>
			<div class="book-title">C Programming Language</div>
			<div class="book-author">Dennis Ritche</div>
			<div class="book-price">$40</div>
			<a href="/buybooks">Buy</a>
		</div>

		<div class="book" data-title="Book 5" data-author="Author 3"
			data-price="30">
			<div class="book-image"
				style="background-image: url(https://m.media-amazon.com/images/I/51pDPOyfLsS._SX198_BO1,204,203,200_QL40_FMwebp_.jpg)"></div>
